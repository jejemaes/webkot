	<template id="admin.menu" name="Admin Menu">
		<ul class="nav" id="side-menu">
			<t t-foreach="$menus" t-as="$m">
				<li t-att-menu-id="$m->id">
		        	<a t-att-href="$url($m->url)">
		        		<t t-if="$m->icon">
			        		<i t-att-class="$m->icon"></i> 
		        		</t>
		        		<t t-esc="$m->name"/>
		        		<t t-if="count($m->children_ids)">
		        			<span class="fa arrow"></span>
		        		</t>
		        	</a>
		        	<t t-if="count($m->children_ids)">
		        		<ul class="nav nav-second-level">
                    		<t t-foreach="$m->children_ids" t-as="$submenu">
                    			<li t-att-menu-id="$submenu->id">
                    				<a t-att-href="$url($submenu->url)"><t t-esc="$submenu->name"/></a>
                    			</li>
                    		</t>
                         </ul>
                	</t>
		    	</li>
			</t>
		</ul>
	</template>
	
